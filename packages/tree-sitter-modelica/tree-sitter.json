{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "modelica",
      "camelcase": "Modelica",
      "title": "Modelica",
      "scope": "source.modelica",
      "file-types": ["mo"],
      "injection-regex": "^modelica$",
      "class-name": "TreeSitterModelica"
    }
  ],
  "metadata": {
    "version": "0.0.0",
    "license": "AGPL-3.0-or-later",
    "description": "Modelica grammar for tree-sitter",
    "authors": [
      {
        "name": "Mohamad Omar Nachawati",
        "email": "mnachawa@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/modelscript/modelscript.git"
    }
  },
  "bindings": {
    "c": false,
    "go": false,
    "node": true,
    "python": false,
    "rust": false,
    "swift": false,
    "zig": false
  }
}
