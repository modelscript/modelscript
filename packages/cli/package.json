    "@npmcli/package-json": "^7.0.0",
    "svgdom": "^0.1.22",
    "tree-sitter": "^0.21.1",
    "xml-formatter": "^3.6.7",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^25.3.2",
    "@types/npmcli__package-json": "^4.0.4",
    "@types/svgdom": "^0.1.2",
    "@types/xml-formatter": "^1.2.0",
    "@types/yargs": "^17.0.34",
    "eslint": "^9.39.1",
    "jiti": "^2.6.1",
    "lerna": "^9.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "rimraf": "^6.1.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  },
  "engines": {
    "node": ">=22"
  }
